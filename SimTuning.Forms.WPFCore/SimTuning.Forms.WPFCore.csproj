<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <CodeAnalysisRuleSet>..\T4M Code Rules.ruleset</CodeAnalysisRuleSet>
    <Authors>Tony Meißner</Authors>
    <Company>tuke productions</Company>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Copyright>tuke productions</Copyright>
    <Description>Ein Tool, um Simson-Begeisterte etwas unter die Arme zu greifen und zu helfen.</Description>
    <NeutralLanguage>de-DE</NeutralLanguage>
    <SignAssembly>false</SignAssembly>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <FileUpgradeFlags>
    </FileUpgradeFlags>
    <UpgradeBackupLocation>
    </UpgradeBackupLocation>
    <OldToolsVersion>Current</OldToolsVersion>
    <PackageId>SimTuning</PackageId>
    <PackageIcon>logo_win.png</PackageIcon>
    <Product>SimTuning</Product>
    <ApplicationIcon>logo_win.ico</ApplicationIcon>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <Version>1.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Views\Home\Tutorial_Berechnungen.xaml.cs" />
    <Compile Remove="Views\Home\Tutorial_dyno.xaml.cs" />
    <Compile Remove="Views\Home\Tutorial_Einstellungen.xaml.cs" />
    <Compile Remove="Views\Home\Tutorial_leistungsdiagnose.xaml.cs" />
    <Compile Remove="Views\Home\Tutorial_main.xaml.cs" />
    <Compile Remove="Views\Home\Tutorial_menues.xaml.cs" />
    <Compile Remove="Views\Home\Tutorial_Overview.xaml.cs" />
  </ItemGroup>

  <ItemGroup>
    <Page Remove="Views\Home\Tutorial_Berechnungen.xaml" />
    <Page Remove="Views\Home\Tutorial_dyno.xaml" />
    <Page Remove="Views\Home\Tutorial_Einstellungen.xaml" />
    <Page Remove="Views\Home\Tutorial_leistungsdiagnose.xaml" />
    <Page Remove="Views\Home\Tutorial_main.xaml" />
    <Page Remove="Views\Home\Tutorial_menues.xaml" />
    <Page Remove="Views\Home\Tutorial_Overview.xaml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Autoupdater.NET.Official" Version="1.6.0" />
    <PackageReference Include="MaterialDesignColors" Version="1.2.6" />
    <PackageReference Include="MaterialDesignThemes" Version="3.1.3" />
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="3.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="MvvmCross" Version="6.4.2" />
    <PackageReference Include="MvvmCross.Platforms.Wpf" Version="6.4.2" />
    <PackageReference Include="NETStandard.Library" Version="2.0.3" />
    <PackageReference Include="OxyPlot.Core" Version="2.0.0" />
    <PackageReference Include="OxyPlot.Wpf" Version="2.0.0" />
    <PackageReference Include="StyleCop.Analyzers" Version="1.1.118">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="System.Windows.Interactivity.WPF" Version="2.0.20525" />
    <PackageReference Include="Xam.Plugin.SimpleAudioPlayer.WPF" Version="1.4.0" />
    <PackageReference Include="XamlAnimatedGif" Version="1.2.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\SimTuning.WPFCore\SimTuning.WPFCore.csproj" />
  </ItemGroup>

  <ItemGroup>
    <ApplicationDefinition Update="App.xaml">
      <SubType>Designer</SubType>
      <Generator>MSBuild:Compile</Generator>
    </ApplicationDefinition>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Views\Dyno\DynoMainView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Views\Motor\MotorUmrechnungenView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Views\Tuning\TuningDataView.xaml.cs">
